{
  "expo": {
    "name": "App-Para",
    "slug": "App-Para",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "permissions": [
        "CAMERA"
      ]
    },
    "ios": {
      "supportsTablet": true,
      "NSCameraUsageDescription": "Esta app necesita acceso a la c√°mara para detectar el pulso"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "permissions": [
          "CAMERA"
        ]
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-audio",
      "expo-video"
    ],
    "extra": {
      "eas": {
        "projectId": "2f86d234-7c04-442d-8086-edcd8cd00d2b"
      }
    }
  }
}
